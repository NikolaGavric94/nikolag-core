{
    "name": "nikolag/core",
    "description": "Nikolag Core package for Laravel",
    "keywords": ["payments", "laravel", "payments laravel integration", "finances"],
    "license": "MIT",
    "type": "laravel-package",
    "authors": [{
        "name": "Nikola Gavric",
        "email": "nikola.gavric94@gmail.com",
        "role": "Developer"
    }],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "3.5.*"
    },
    "autoload": {
        "psr-4": {
            "Nikolag\\Core\\": "src/",
            "Nikolag\\Core\\Abstracts\\": "src/abstracts",
            "Nikolag\\Core\\Contracts\\": "src/contracts",
            "Nikolag\\Core\\Exceptions\\": "src/exceptions",
            "Nikolag\\Core\\Facades\\": "src/facades",
            "Nikolag\\Core\\Models\\": "src/models",
            "Nikolag\\Core\\Providers\\": "src/providers",
            "Nikolag\\Core\\Utils\\": "src/utils"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nikolag\\Core\\": "src/",
            "Nikolag\\Core\\Abstracts\\": "src/abstracts",
            "Nikolag\\Core\\Contracts\\": "src/contracts",
            "Nikolag\\Core\\Exceptions\\": "src/exceptions",
            "Nikolag\\Core\\Facades\\": "src/facades",
            "Nikolag\\Core\\Models\\": "src/models",
            "Nikolag\\Core\\Providers\\": "src/providers",
            "Nikolag\\Core\\Utils\\": "src/utils"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nikolag\\Core\\Providers\\MigrationServiceProvider",
                "Nikolag\\Core\\Providers\\CoreServiceProvider"
            ],
            "facades": [
                "Nikolag\\Core\\Facades\\CoreService"
            ]
        }
    }
}